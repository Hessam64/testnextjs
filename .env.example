PORT=4000
CORS_ORIGIN=http://localhost:3000
DATABASE_URL=postgresql://user:password@host:5432/database
# Set to "off" to disable SSL, "strict" to enforce cert validation, defaults to allowing self-signed
DB_SSL=prefer
# Force IPv4 connections (useful if your host advertises IPv6 but the platform cannot reach it)
DB_FORCE_IPV4=true
